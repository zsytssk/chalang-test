
name= {ASCII_ALPHA~char*}      // for instance, "1362 + 12"
char = { ASCII_ALPHANUMERIC |  "_"}
WHITESPACE = _{ " " | "\t" }
END_LINE = _{ ";" | "\n" }
Define = _ {"let" | "const"}
// statement = {"let" ~ WHITESPACE+ ~ char+ ~ WHITESPACE+ ~ "=" ~ WHITESPACE+ ~ char+ ~";"}
statement = {Define ~ name ~ "=" ~ NUMBER+ ~ END_LINE}